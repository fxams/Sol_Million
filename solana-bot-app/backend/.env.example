PORT=8787

# Frontend origin for CORS (local dev default)
FRONTEND_ORIGIN=http://localhost:3000

# Cluster selection: mainnet-beta or devnet
CLUSTER=mainnet-beta

# Helius endpoints (recommended: include your api-key in the URL query string)
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
HELIUS_WS_URL=wss://mainnet.helius-rpc.com/?api-key=YOUR_KEY

# Jito block engine base URL (mainnet only)
JITO_BLOCK_ENGINE_URL=https://mainnet.block-engine.jito.wtf

# Pump.fun bonding curve program id (for pre-migration sniping).
# Set this if you want to use "Pump.fun pre-migration" mode.
PUMPFUN_PROGRAM_ID=

